{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "strict": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "baseUrl": "./",
        "outDir": "./dist",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "@/*": [
                "app/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}
